{
	"env": {
	  "node": true,
	  "browser": true,
	  "es6": true,
	  "jest": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
	  "ecmaVersion": 2018,
	  "sourceType": "module"
	},
	"rules": {
	  "comma-dangle": ["error", "always-multiline"],
	  "indent": ["error", 2],
	  "quotes": ["error", "single"],
	  "semi": ["error", "always"]
	}
  }
	